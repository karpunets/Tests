{
  "request": {
    "name": "$name",
    "system": false,
    "templateRole": "$templateRole",
    "sessionLiveTimeSec": 300,
    "group": {
      "groupId": "$groupId"
    }
  },


  "schema": {
    "name": "$name",
    "sessionLiveTimeSec": 300,
    "group": {
      "name": {"type":"string"},
      "cid": {"type":"number"},
      "groupId": "$groupId"
    },
    "templateRole": "$templateRole",
    "system": false,
    "roleId": {"type":"string"}
  },


  "response_schema":
          {
    "name": {"allowed": ["$name"]},
    "sessionLiveTimeSec": {"allowed": [300]},
    "group":
        {
            "type": "dict",
            "schema": {
                "name": {"type": "string"},
                "cid": {"type": "integer"},
                "groupId": {"allowed": ["$groupId"]}
            }
        },
    "templateRole": {"allowed": ["$templateRole"]},
    "system": {"allowed": [false]},
    "roleId": {"type": "string"}}
}




