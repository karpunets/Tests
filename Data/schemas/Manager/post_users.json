{
  "request": {
    "agentId": "$agentId",
    "login": "$login",
    "loginAD": "$ADlogin",
    "lname": "$lname",
    "password":"$password",
    "fname": "$fname",
    "pname": "$pname",
    "email": "$email",
    "phone": "$phone",
    "fax": "$fax",
    "enabled": {"default": true, "value": "$enabled"},
    "deleted": {"default": false, "value": "$deleted"},
    "roles": [
      {
        "roleId": "$roleId"
      }
    ],
    "userGroupRoles": "$userGroupRoles"
  },
  "schema": {
    "userId": {"type": "string"},
    "agentId": {"allowed": ["$agentId"]},
    "login": {"allowed": ["$login"]},
    "loginAD": {"allowed": ["$ADlogin"]},
    "lname": {"allowed": ["$lname"]},
    "fname": {"allowed": ["$fname"]},
    "pname": {"allowed": ["$pname"]},
    "email": {"allowed": ["$email"]},
    "phone": {"allowed": ["$phone"]},
    "fax": {"allowed": ["$fax"]},
    "dateCreate": {"type":"number"},
    "enabled": {"allowed": ["$enabled"]},
    "deleted": {"allowed": ["$deleted"]},
    "groups": {"type": "list"},
    "roles": {"type": "list"},
    "userGroupRoles": {"type":"list"},
    "domain": null,
    "imgUrl": null
  }
}